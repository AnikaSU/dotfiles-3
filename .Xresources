! Use a nice truetype font and size by default... 
UXTerm*faceName: FantasqueSansMono Nerd Font Mono
UXTerm*faceSize: 11
xterm*selectToClipboard: true

! X colors.
! Generated by 'wal'
*foreground:        #e6c9c4
*background:        #0a0709
*.foreground:       #e6c9c4
*.background:       #0a0709
emacs*foreground:   #e6c9c4
emacs*background:   #0a0709
URxvt*foreground:   #e6c9c4
XTerm*foreground:   #e6c9c4
UXTerm*foreground:  #e6c9c4
URxvt*background:   [100]#0a0709
XTerm*background:   #0a0709
UXTerm*background:  #0a0709
URxvt*cursorColor:  #e6c9c4
XTerm*cursorColor:  #e6c9c4
UXTerm*cursorColor: #e6c9c4
URxvt*borderColor:  [100]#0a0709

! Colors 0-15.
*.color0: #0a0709
*color0:  #0a0709
*.color1: #D08A6F
*color1:  #D08A6F
*.color2: #4E2189
*color2:  #4E2189
*.color3: #786384
*color3:  #786384
*.color4: #D1328C
*color4:  #D1328C
*.color5: #91728C
*color5:  #91728C
*.color6: #AF8C97
*color6:  #AF8C97
*.color7: #e6c9c4
*color7:  #e6c9c4
*.color8: #a18c89
*color8:  #a18c89
*.color9: #D08A6F
*color9:  #D08A6F
*.color10: #4E2189
*color10:  #4E2189
*.color11: #786384
*color11:  #786384
*.color12: #D1328C
*color12:  #D1328C
*.color13: #91728C
*color13:  #91728C
*.color14: #AF8C97
*color14:  #AF8C97
*.color15: #e6c9c4
*color15:  #e6c9c4

! Black color that will not be affected by bold highlighting.
*.color66: #0a0709
*color66:  #0a0709

! Xclock colors.
XClock*foreground: #e6c9c4
XClock*background: #0a0709
XClock*majorColor:  rgba:e6/c9/c4/ff
XClock*minorColor:  rgba:e6/c9/c4/ff
XClock*hourColor:   rgba:e6/c9/c4/ff
XClock*minuteColor: rgba:e6/c9/c4/ff
XClock*secondColor: rgba:e6/c9/c4/ff

! Set depth to make transparency work.
URxvt*depth: 32
! Every shell is a login shell by default (for inclusion of all necessary environment variables)
xterm*loginshell: true

! I like a LOT of scrollback...
xterm*savelines: 16384

! double-click to select whole URLs :D
xterm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48

XTerm*selectToClipboard:true
! right hand side scrollbar...
xterm*rightScrollBar: false
xterm*ScrollBar: false

! stop output to terminal from jumping down to bottom of scroll again
xterm*scrollTtyOutput: false

